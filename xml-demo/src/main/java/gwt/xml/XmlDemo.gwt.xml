<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.8.2//EN"
        "http://www.gwtproject.org/doctype/2.8.2/gwt-module.dtd">
<module rename-to="demo">
    <inherits name="gwt.xml.Xml"/>
    <inherits name="elemental2.dom.Dom"/>
    <inherits name="org.dominokit.domino.ui.DominoUI"/>

    <!--  -->
    <collapse-all-properties/>

    <!-- Enable I18N -->
    <inherits name="com.google.gwt.i18n.I18N"/>
    <extend-property name="locale" values="en,zh"/>
    <set-property-fallback name="locale" value="en"/>
    <set-property name="locale" value="en,zh"/>
    <set-configuration-property name="locale.cookie" value="locale"/>
    <set-configuration-property name="locale.useragent" value="Y"/>

    <set-property name="user.agent" value="safari"/>

    <entry-point class="gwt.xml.client.XmlDemo"/>

    <source path="client"/>
    <source path="shared"/>
    <super-source path="super"/>

    <!-- <add-linker name="sso"/> -->
</module>
